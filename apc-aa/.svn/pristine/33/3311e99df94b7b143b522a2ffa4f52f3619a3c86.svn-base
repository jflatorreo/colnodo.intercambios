/* -- common -- */
.aa-table table {
    border: 1px solid #86682D;
    box-shadow: inset 0 0 16px #96805D;
    color: #000;
    padding: 15px;
    border-collapse: collapse;
}
.aa-table th {
    text-align: left;
    padding: 7px 10px 4px;
    font-size: 0.9em;
    background-color: #A8977A;
    color: #FFF;
    text-shadow: 1px 1px 1px #464545;
    font-weight: normal;
}
.aa-table td {
    border-top: 1px solid #BEB29F;
    padding: 5px 10px;
    font-size: 0.85em;
}
.aa-table tr:hover {
    background-color: #F0E8DC;
}
[class^="aa-button-"] {
    box-shadow: #000 0px 0px 3px;
    padding: 1px 8px 2px 18px;
    display: inline-block;
    border-radius: 7px;
    background-color: #FFF;
    text-decoration: none;
    color: #812A0F;
}
[class^="aa-button-"]:hover {
    background-color: #FAF2D9;
    color: #494949;
    box-shadow: 0 0 6px #000;
}
.aa-button-edit {
    background: #FFF url(data:image/png;base64,) no-repeat 2px center;
}
.aa-button-delete {
    background: #FFF url(data:image/png;base64,) no-repeat 5px center;
}

/* message-boxes css */
#aa-message-box {
    width: 100%;
    text-align: center;
    position: absolute;
    z-index: 100;
}
#aa-message-box span {
    display: inline-block;
    min-width: 20%;
    max-width: 40%;
    margin: 150px 25%;
    box-shadow: 0 0 20px #000;
    border: solid 1px #5A5A5A;
    padding: 20px;
    text-align: left;
}
#aa-message-box.aa-err span {
    background-color: #FFE0DB;
}
#aa-message-box.aa-info span {
    background-color: #FFF9BA;
}
#aa-message-box.aa-ok span {
    background-color: #E2FAC4;
}

/* translate in inputform */
.aa-langtrans-label {
    padding: 6px 5px 7px 5px;
    line-height: 1;
    display: inline-block;
    border-radius: 7px 7px 0 0;
    background-color: #A8977A;
    text-decoration: none;
    color: #FFF;
    border: 1px solid #91856E;
    border-bottom: none;
    margin-top: 4px;
    margin-right: 5px;
    text-shadow: 0 0 1px #464545;
    font-size: 8pt;
}
.aa-langtrans-tbtn {
     display: inline-block;
     background-color: #d5c2a4;
     font-size: 11px;
     border: solid 1px #a8977a;
     border-radius: 7px;
     padding: 2px 5px;
}
.aa-langtrans-tbtn:hover {
    background-color: #B39E7D;
    cursor:pointer;
}
.aa-langdiv input[type=text], .aa-langdiv textarea  {
    display: block;
}
.aa-langswitch {
    height: 12px;
    border-bottom: 1px solid #CFC0A8;
    margin-bottom: 12px;
}
.aa-langswitch input[type=button] {
    box-shadow: #53452e 0px 0px 2px;
    padding: 0 2px 2px 2px;
    display: inline-block;
    border-radius: 7px;
    background-color: #D1C1A7;
    text-decoration: none;
    color: #FFFFFF;
    border: none;
    margin-top: 4px;
    text-shadow: 0 0 1px #464545;
}
.aa-langswitch input[type=button].aa-open,
.aa-langswitch input[type=button].aa-filled.aa-open,
.aa-langswitch input[type=button].aa-filled.aa-open:hover {
    display: none;
}
.aa-langswitch input[type=button].aa-filled {
    box-shadow: #000 0px 0px 2px;
    background-color: #A8977A;
    color: #FFF;
    text-shadow: 0 0 1px #464545;
}
.aa-langswitch input[type=button]:hover {
    background-color: #FAF2D9;
    color: #494949;
    box-shadow: 0 0 6px #000;
    text-shadow: none;
}
.aa-langswitch input[type=button].aa-open:hover {
    background-color: #cab89c;
    box-shadow: #000 0px 0px 2px;
}
.aa-langswitch input[type=button].aa-open.aa-filled:hover {
    background-color: #A8977A;
    box-shadow: #000 0px 0px 2px;
}
.aa-langswitch input[type=button].aa-langall {
    float: right;
    text-shadow: 0 0 1px white;
    box-shadow: 0 0 2px #589868;
    color: #441717;
    padding: 0 7px 2px 17px;
    background:  #589868
        url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAGCAYAAAD68A/GAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAGFJREFUeNpiDHbV82NgYFgHxMwMuEEBE5DYBMQ1eBQtAOKJTFBOB1QAHVwA4iwQgwlJMB2I9yHx3wFxEBB/R1f4C4gjgPgOEP8F4mggvg+TZEGz6jUQ+wCxBxDvQJYACDAAikQQ/JwyWB0AAAAASUVORK5CYII=)
        no-repeat 4px center;
}

.aa-langswitch input[type=button].aa-langall.aa-open {
