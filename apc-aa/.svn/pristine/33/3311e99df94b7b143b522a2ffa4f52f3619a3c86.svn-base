/* pager - Item manager */
.pager {
    clear: both;
    padding: 30px 0 30px 0;
    color: #0000;
    letter-spacing: -4px;
}
.pager a, .pager .active {
    background-color: #DAE8E1;
    padding: 3px 7px;
    letter-spacing: normal;
    margin: 0 1px;
    color: #000000;
    text-decoration: none;
}
.pager .dots {
    background-color: transparent;
    color: #414141;
    letter-spacing: normal;
}
.pager a:hover, .pager .active {
    background-color: #589868;
    color: white;
}
.pager:empty {
    display: none;
}
.pager .found {
    color: #414141;
    letter-spacing: normal;
    margin-left: 10px;
    font-size: 11px;
}

/* -------------------- Sitemodule CSS - begin --------------------------- */
#sitetree {
    font-size: 8pt;
    color: #000000;
    width: 200px;
    padding-right: 12px;
    vertical-align: top;
}

#sitetree div {
    border-right: 1px solid #CEE0D3;
    border-left: 1px solid #CEE0D3;
    background: url(modules/site/images/i_bg.gif) repeat-y;
}

#sitetree .spot  {
    color: #000000;
    text-decoration: none;
    margin: 0px 0px 0px 0px;
    background-color: #D8E7DC;
    border-top: none;
    border-bottom:  1px solid #A8C8B0;
    padding-left: 2px;
    display: block;
}

#sitetree div.choice, #sitetree div.lastchoice {
    padding: 0px 5px 5px 21px;
}

#sitetree div.lastchoice {
    background-image: url(modules/site/images/s_bg.gif);
    border-bottom: 1px solid #D8E7DC;
}

#sitetree div.conditions {
    border-top: 1px solid #D8E7DC;
    border-left: 1px solid #D8E7DC;
    background-image: none;
    padding-left: 1px;
}

#sitetree div.conditions a, #sitetree div.variables {
    color: #000;
    text-decoration: none;
}

#sitetree div.conditions a {
    color: #197119;
}

#sitetree div.variables {
    padding-bottom: 8px;
    padding-left:   15px;
}

.choice img, .lastchoice img {
    margin: 0px 0px 0px -21px;
    padding: 0px;
    float: left;
    border: none;
}

img.eye {
    margin: 0px;
    padding: 0px;
    float: right;
    border: none;
}

#sitetree a.spot:hover {
    background-color: #619A70;
    color: #000000;
}

#sitetree div .selected {
    margin: 0px 1px 0px 0px;
    padding: 0px;
    border: none;
}

#sitetree div .selected a {
    background-color: #456D50;
    color: #FFFFFF;
}

/* -------------------- Sitemodule CSS - end ----------------------------- */

/* -- live editing  */
select.live, input.live, textarea.live {
    border: 1px solid #589868;
}

div.ajax_value {       /* edit.png */
    background: url(data:image/png;base64,) no-repeat scroll right bottom transparent;
    padding-right: 20px;
}
div.ajax_value:hover { /* edit-a.png */
    background-image: url(data:image/png;base64,);
}

div.ajax_value[data-aa-edited="1"],
div.ajax_value[data-aa-edited="1"]:hover {
    background: none;
}

input.save-button, input.cancel-button {
    border: 1px solid #D6C4A9;
    border-right-color: #A0988D;
    border-bottom-color: #A0988D;
    padding-left: 15px;
    font-size: 9px;
    text-transform: lowercase;
}
