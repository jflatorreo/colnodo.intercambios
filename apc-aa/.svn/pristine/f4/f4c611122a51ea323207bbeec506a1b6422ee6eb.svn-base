/* pager - Item manager */
.pager {
    clear: both;
    padding: 30px 0 30px 0;
    color: transparent;
    letter-spacing: -4px;
}
.pager a, .pager .active {
    background-color: #D5C2A4;
    padding: 3px 7px;
    letter-spacing: normal;
    margin: 0 1px;
    color: #000000;
    text-decoration: none;
}
.pager .dots {
    background-color: transparent;
    color: #414141;
    letter-spacing: normal;
}
.pager a:hover, .pager .active {
    background-color: #B39E7D;
    color: white;
}
.pager:empty {
    display: none;
}
.pager .found {
    color: #414141;
    letter-spacing: normal;
    margin-left: 10px;
    font-size: 11px;
}

/* -------------------- Sitemodule CSS - begin --------------------------- */
#sitetree {
    font-size: 8pt;
    color: #000000;
    width: 200px;
    padding-right: 12px;
    vertical-align: top;
}

#sitetree div {
    border-right: 1px solid #EBDABE; /* i_bg.gif */
    background: url(data:image/gif;base64,R0lGODlhFwAEAKECAM7g02tra////////yH5BAEAAAAALAAAAAAXAAQAAAIMhH+By60N2YsUTFoAADs=) repeat-y;
}

#sitetree .spot  {
    color: #000000;
    text-decoration: none;
    margin: 0 0 0 0;
    background-color: #F5F0E7;
    border-top: none;
    border-bottom:  1px solid #EBDABE;
    padding-left: 2px;
    display: block;
}

#sitetree div.choice, #sitetree div.lastchoice {
    padding: 0 5px 5px 21px;
}

#sitetree div.lastchoice { /* s_bg.gif */
    background-image: url(data:image/gif;base64,R0lGODlhFwAEAPABAM7g0wAAACH5BAUAAAEALAAAAAAXAAQAAAILRI6pG8a/HJyN0gIAOw==);
    border-bottom: 1px solid #EBDABE;
}

#sitetree div.conditions {
    border-top: 1px solid #EBDABE;
    border-left: 1px solid #EBDABE;
    background: #EBDABE none;
}

#sitetree div.conditions a, #sitetree div.variables {
    color: #000;
    text-decoration: none;
}

#sitetree div.conditions a {
    color: #584011;
}

#sitetree div.variables {
    padding-bottom: 8px;
    padding-left:   15px;
    color: #584011;
}

.choice img, .lastchoice img {
    margin: 0 0 0 -21px;
    padding: 0;
    float: left;
    border: none;
}

img.eye {
    margin: 0;
    padding: 0;
    float: right;
    border: none;
}

#sitetree a.spot:hover {
    background-color: #584011;
    color: #FFF;
}

#sitetree div.selected {
    margin: 0 1px 0 0;
    padding: 0;
    border: none;
}

#sitetree div.selected a {
    background-color: #584011;
    color: #FFF;
}

/* -------------------- Sitemodule CSS - end ----------------------------- */

/* -- live editing  */
select.live, input.live, textarea.live {
    border: 1px solid #EBDABE;
}

select.updating, input.updating, textarea.updating {
    outline: 1px solid #FF0000;
}

div.ajax_value {       /* edit.png */
    background: url(data:image/png;base64,) no-repeat scroll right bottom transparent;
    padding-right: 20px;
}
div.ajax_value:hover { /* edit-a.png */
    background-image: url(data:image/png;base64,);
}

div.ajax_value[data-aa-edited="1"],
div.ajax_value[data-aa-edited="1"]:hover {
    background: none;
}

input.save-button, input.cancel-button {
    border: 1px solid #D6C4A9;
    border-right-color: #A0988D;
    border-bottom-color: #A0988D;
    padding-left: 15px;
    font-size: 9px;
    text-transform: lowercase;
}
input.save-button {   /* save.png */
     background : url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQAgMAAABinRfyAAAADFBMVEUAAAAzdGmcq6RijYMK9IDiAAAABHRSTlMAx8fHpsjwxQAAADNJREFUCNdjQADR+isMDFLOkiBCyoFBygFEhEYBiVWrHBikQ+McGCQZ2DAI0f9fHRCGAACp6wrUxFiLHgAAAABJRU5ErkJggg==) no-repeat #DBDAD9 0 -2px;
}
input.cancel-button {
     background-color: #DBDAD9;
     padding-left: 2px;
}



/* -- common -- */
.aa-table table {
    border: 1px solid #86682D;
    box-shadow: inset 0 0 16px #96805D;
    color: #000;
    padding: 15px;
    border-collapse: collapse;
}
.aa-table th {
    text-align: left;
    padding: 7px 10px 4px;
    font-size: 0.9em;
    background-color: #A8977A;
    color: #FFF;
    text-shadow: 1px 1px 1px #464545;
    font-weight: normal;
}
.aa-table td {
    border-top: 1px solid #BEB29F;
    padding: 5px 10px;
    font-size: 0.85em;
}
.aa-table tr:hover {
    background-color: #F0E8DC;
}
[class^="aa-button-"] {
    box-shadow: #000 0 0 3px;
    padding: 1px 8px 2px 18px;
    display: inline-block;
    border-radius: 7px;
    background-color: #FFF;
    text-decoration: none;
    color: #812A0F;
}
[class^="aa-button-"]:hover {
    background-color: #FAF2D9;
    color: #494949;
    box-shadow: 0 0 6px #000;
}
.aa-button-edit {
    background: #FFF url(data:image/png;base64,) no-repeat 2px center;
}
.aa-button-delete {
    background: #FFF url(data:image/png;base64,) no-repeat 5px center;
}

/* message-boxes css */
#aa-message-box {
    width: 100%;
    text-align: center;
    position: absolute;
    z-index: 100;
}
#aa-message-box span {
    display: inline-block;
    min-width: 20%;
    max-width: 40%;
    margin: 150px 25%;
    box-shadow: 0 0 20px #000;
    border: solid 1px #5A5A5A;
    padding: 20px;
    text-align: left;
}
#aa-message-box.aa-err span {
    background-color: #FFE0DB;
}
#aa-message-box.aa-info span {
    background-color: #FFF9BA;
}
#aa-message-box.aa-ok span {
    background-color: #E2FAC4;
}

/* translate in inputform */
.aa-langtrans-label {
    padding: 6px 5px 7px 5px;
    line-height: 1;
    display: inline-block;
    border-radius: 7px 7px 0 0;
    background-color: #A8977A;
    text-decoration: none;
    color: #FFF;
    border: 1px solid #91856E;
    border-bottom: none;
    margin-top: 4px;
    margin-right: 5px;
    text-shadow: 0 0 1px #464545;
    font-size: 8pt;
}

.aa-widget-fld .aa-langtrans-label {
    padding: 2px 3px 3px 3px;
    border-radius: 5px 0 0 5px;
    color: #FFF;
    border-bottom: 1px solid #91856E;
    margin: 0;
}

.aa-langtrans-tbtn {
    display: inline-block;
    background-color: #d5c2a4;
    font-size: 11px;
    border: solid 1px #a8977a;
    border-radius: 7px;
    padding: 2px 5px;
}
.aa-langtrans-tbtn:hover {
    background-color: #B39E7D;
    cursor:pointer;
}

.aa-langdiv textarea  {
    display: block;
}
.aa-langswitch {
    height: 12px;
    border-bottom: 1px solid #CFC0A8;
    margin-bottom: 12px;
}
.aa-langswitch input[type=button] {
    box-shadow: #53452e 0 0 2px;
    padding: 0 2px 2px 2px;
    display: inline-block;
    border-radius: 7px;
    background-color: #D1C1A7;
    text-decoration: none;
    color: #FFFFFF;
    border: none;
    margin-top: 4px;
    text-shadow: 0 0 1px #464545;
}
.aa-langswitch input[type=button].aa-open,
.aa-langswitch input[type=button].aa-filled.aa-open,
.aa-langswitch input[type=button].aa-filled.aa-open:hover {
    display: none;
}
.aa-langswitch input[type=button].aa-filled {
    box-shadow: #000 0 0 2px;
    background-color: #A8977A;
    color: #FFF;
    text-shadow: 0 0 1px #464545;
}
.aa-langswitch input[type=button]:hover {
    background-color: #FAF2D9;
    color: #494949;
    box-shadow: 0 0 6px #000;
    text-shadow: none;
}
.aa-langswitch input[type=button].aa-open:hover {
    background-color: #cab89c;
    box-shadow: #000 0 0 2px;
}
.aa-langswitch input[type=button].aa-open.aa-filled:hover {
    background-color: #A8977A;
    box-shadow: #000 0 0 2px;
}
.aa-langswitch input[type=button].aa-langall {
    float: right;
    text-shadow: 0 0 1px white;
    box-shadow: 0 0 2px #EBDABE;
    color: #441717;
    padding: 0 7px 2px 17px;
    background:  #EBDABE
        url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAGCAYAAAD68A/GAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAGFJREFUeNpiDHbV82NgYFgHxMwMuEEBE5DYBMQ1eBQtAOKJTFBOB1QAHVwA4iwQgwlJMB2I9yHx3wFxEBB/R1f4C4gjgPgOEP8F4mggvg+TZEGz6jUQ+wCxBxDvQJYACDAAikQQ/JwyWB0AAAAASUVORK5CYII=)
